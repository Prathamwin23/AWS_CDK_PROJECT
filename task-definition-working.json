[
  {
    "name": "django-app",
    "image": "516268691462.dkr.ecr.ap-south-1.amazonaws.com/dev/django-app:working",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8000,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "DB_NAME",
        "value": "classicappdb"
      },
      {
        "name": "DEBUG",
        "value": "true"
      },
      {
        "name": "DB_HOST",
        "value": "dev-classic-app-db.cfqe002kq5rb.ap-south-1.rds.amazonaws.com"
      },
      {
        "name": "DB_PORT",
        "value": "3306"
      },
      {
        "name": "DB_USER",
        "value": "classicadmin"
      },
      {
        "name": "SECRET_KEY",
        "value": "django-insecure-change-this-in-production-123456789abcdef"
      },
      {
        "name": "DB_PASSWORD",
        "value": "NewSecurePassword123!"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/dev/django-app",
        "awslogs-region": "ap-south-1",
        "awslogs-stream-prefix": "django-app"
      }
    }
  }
]