{
  "family": "dev-django-app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::516268691462:role/dev-EcsStack-TaskExecutionRole250D2532-X4yUxPn7gkD1",
  "taskRoleArn": "arn:aws:iam::516268691462:role/dev-EcsStack-TaskRole30FC0FBB-xy9BRs3jMFSH",
  "containerDefinitions": [
    {
      "name": "django-app",
      "image": "516268691462.dkr.ecr.ap-south-1.amazonaws.com/dev/django-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_NAME",
          "value": "classicappdb"
        },
        {
          "name": "DEBUG",
          "value": "true"
        },
        {
          "name": "DB_HOST",
          "value": "dev-classic-app-db.cfqe002kq5rb.ap-south-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "3306"
        },
        {
          "name": "DB_USER",
          "value": "classicadmin"
        },
        {
          "name": "SECRET_KEY",
          "value": "django-insecure-change-this-in-production-123456789abcdef"
        }
      ],
      "secrets": [
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:516268691462:secret:dev/classic-app/db-credentials-LIUcuK:password::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/dev/django-app",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "django-app"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health/ || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}